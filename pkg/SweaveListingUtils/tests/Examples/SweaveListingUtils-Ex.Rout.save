[1081] "#### new from version 2.0: Mixing Distributions"                                                                                        
[1082] ""                                                                                                                                       
[1083] "################################"                                                                                                       
[1084] "##"                                                                                                                                     
[1085] "## Mixing Distribution classes "                                                                                                        
[1086] "##"                                                                                                                                     
[1087] "################################"                                                                                                       
[1088] ""                                                                                                                                       
[1089] "setClass(\"UnivarMixingDistribution\","                                                                                                 
[1090] "            representation = representation(mixCoeff = \"numeric\","                                                                    
[1091] "                             mixDistr = \"UnivarDistrList\","                                                                           
[1092] "                             gaps = \"OptionalMatrix\","                                                                                
[1093] "                             support = \"numeric\","                                                                                    
[1094] "                             Symmetry = \"DistributionSymmetry\","                                                                      
[1095] "                             .withArith = \"logical\","                                                                                 
[1096] "                             .withSim = \"logical\","                                                                                   
[1097] "                             .logExact = \"logical\","                                                                                  
[1098] "                             .lowerExact = \"logical\""                                                                                 
[1099] "                             ),"                                                                                                        
[1100] "            prototype = prototype(mixCoeff = 1, "                                                                                       
[1101] "                                  mixDistr = new(\"UnivarDistrList\"),"                                                                 
[1102] "                                  gaps = NULL,"                                                                                         
[1103] "                                  support = numeric(0),"                                                                                
[1104] "                                  Symmetry = new(\"NoSymmetry\"),"                                                                      
[1105] "                                 .withArith = FALSE,"                                                                                   
[1106] "                                 .withSim = FALSE,"                                                                                     
[1107] "                                 .logExact = TRUE,"                                                                                     
[1108] "                                 .lowerExact = TRUE"                                                                                    
[1109] "                                  ),"                                                                                                   
[1110] "            contains = \"UnivariateDistribution\","                                                                                     
[1111] "            validity = function(object){"                                                                                               
[1112] "                if(any(object@mixCoeff< -.Machine$double.eps) || "                                                                      
[1113] "                   sum(object@mixCoeff)>1+.Machine$double.eps)"                                                                         
[1114] "                   stop(\"mixing coefficients are no probabilities\")"                                                                  
[1115] "                return(TRUE)"                                                                                                           
[1116] "            })"                                                                                                                         
[1117] ""                                                                                                                                       
[1118] "setClass(\"UnivarLebDecDistribution\","                                                                                                 
[1119] "            representation = representation(mixCoeff = \"numeric\","                                                                    
[1120] "                             mixDistr = \"UnivarDistrList\"),"                                                                          
[1121] "            prototype = prototype(mixCoeff = c(\"acWeight\" = 1, "                                                                      
[1122] "                                               \"discreteWeight\" = 0),"                                                                
[1123] "                                  mixDistr = new(\"UnivarDistrList\","                                                                  
[1124] "                                              list(\"acPart\" = new(\"Norm\"),"                                                         
[1125] "                                                   \"discretePart\" = new(\"Dirac\")"                                                   
[1126] "                                                   )"                                                                                   
[1127] "                                  )),"                                                                                                  
[1128] "            contains = \"UnivarMixingDistribution\","                                                                                   
[1129] "            validity = function(object){"                                                                                               
[1130] "                if (length(object@mixCoeff)!=2)"                                                                                        
[1131] "                    stop(\"number of mixing components is not 2\")"                                                                     
[1132] "                if (!is(object@mixDistr[[1]], \"AbscontDistribution\"))"                                                                
[1133] "                    stop(\"first component must be absolutely continuous\")"                                                            
[1134] "                if (!is(object@mixDistr[[2]], \"DiscreteDistribution\"))"                                                               
[1135] "                    stop(\"second component must be discrete\")"                                                                        
[1136] "                return(TRUE)"                                                                                                           
[1137] "            })"                                                                                                                         
[1138] ""                                                                                                                                       
[1139] "setClass(\"AffLinUnivarLebDecDistribution\","                                                                                           
[1140] "          representation = representation(a = \"numeric\", b = \"numeric\","                                                            
[1141] "          X0 = \"UnivarLebDecDistribution\"),"                                                                                          
[1142] "          prototype = prototype(a = 1, b = 0, "                                                                                         
[1143] "                                X0 = new(\"UnivarLebDecDistribution\")),"                                                               
[1144] "          contains = \"UnivarLebDecDistribution\""                                                                                      
[1145] "          )"                                                                                                                            
[1146] ""                                                                                                                                       
[1147] "         "                                                                                                                              
[1148] "setClassUnion(\"UnivDistrListOrDistribution\","                                                                                         
[1149] "               c(\"UnivarDistrList\",\"UnivariateDistribution\"))"                                                                      
[1150] ""                                                                                                                                       
[1151] "setClass(\"CompoundDistribution\", representation=representation("                                                                      
[1152] "             NumbOfSummandsDistr = \"DiscreteDistribution\","                                                                           
[1153] "             SummandsDistr = \"UnivDistrListOrDistribution\"),"                                                                         
[1154] "          prototype=prototype(NumbOfSummandsDistr = new(\"Pois\"),"                                                                     
[1155] "              SummandsDistr=new(\"Norm\")),"                                                                                            
[1156] "          contains = \"UnivarMixingDistribution\""                                                                                      
[1157] "         )"                                                                                                                             
[1158] ""                                                                                                                                       
[1159] "################################"                                                                                                       
[1160] "##"                                                                                                                                     
[1161] "## virtual Distribution class Unions "                                                                                                  
[1162] "##"                                                                                                                                     
[1163] "################################"                                                                                                       
[1164] ""                                                                                                                                       
[1165] "setClassUnion(\"AcDcLcDistribution\", c(\"AbscontDistribution\","                                                                       
[1166] "               \"DiscreteDistribution\", \"UnivarLebDecDistribution\","                                                                 
[1167] "               \"CompoundDistribution\"))"                                                                                              
[1168] ""                                                                                                                                       
[1169] "setClassUnion(\"AffLinDistribution\", c(\"AffLinAbscontDistribution\","                                                                 
[1170] "               \"AffLinDiscreteDistribution\", \"AffLinUnivarLebDecDistribution\"))"                                                    
[1171] ""                                                                                                                                       
