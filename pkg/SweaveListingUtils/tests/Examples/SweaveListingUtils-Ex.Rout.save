[1088] "setClass(\"UnivarMixingDistribution\","                                                                                                 
[1089] "            representation = representation(mixCoeff = \"numeric\","                                                                    
[1090] "                             mixDistr = \"UnivarDistrList\","                                                                           
[1091] "                             gaps = \"OptionalMatrix\","                                                                                
[1092] "                             support = \"numeric\","                                                                                    
[1093] "                             Symmetry = \"DistributionSymmetry\","                                                                      
[1094] "                             .withArith = \"logical\","                                                                                 
[1095] "                             .withSim = \"logical\","                                                                                   
[1096] "                             .logExact = \"logical\","                                                                                  
[1097] "                             .lowerExact = \"logical\""                                                                                 
[1098] "                             ),"                                                                                                        
[1099] "            prototype = prototype(mixCoeff = 1, "                                                                                       
[1100] "                                  mixDistr = new(\"UnivarDistrList\"),"                                                                 
[1101] "                                  gaps = NULL,"                                                                                         
[1102] "                                  support = numeric(0),"                                                                                
[1103] "                                  Symmetry = new(\"NoSymmetry\"),"                                                                      
[1104] "                                 .withArith = FALSE,"                                                                                   
[1105] "                                 .withSim = FALSE,"                                                                                     
[1106] "                                 .logExact = TRUE,"                                                                                     
[1107] "                                 .lowerExact = TRUE"                                                                                    
[1108] "                                  ),"                                                                                                   
[1109] "            contains = \"UnivariateDistribution\","                                                                                     
[1110] "            validity = function(object){"                                                                                               
[1111] "                if(any(object@mixCoeff< -.Machine$double.eps) || "                                                                      
[1112] "                   sum(object@mixCoeff)>1+.Machine$double.eps)"                                                                         
[1113] "                   stop(\"mixing coefficients are no probabilities\")"                                                                  
[1114] "                return(TRUE)"                                                                                                           
[1115] "            })"                                                                                                                         
[1116] ""                                                                                                                                       
