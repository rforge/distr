 [514] "## Class: Laplace or Double Exponential distribution"                                                                                   
 [515] "setClass(\"DExp\","                                                                                                                     
 [516] "          prototype = prototype("                                                                                                       
 [517] "                      r = function(n){"                                                                                                 
 [518] "                              (2*rbinom(n ,size = 1, prob = 0.5)-1) *"                                                                  
 [519] "                                 rexp(n, rate = 1)"                                                                                     
 [520] "                              },"                                                                                                       
 [521] "                      d = function(x, log = FALSE)"                                                                                     
 [522] "                              { d0 <-  dexp(abs(x), rate = 1, log = log)"                                                               
 [523] "                                d0 <- if (log) d0-log(2) else d0 <- d0 / 2"                                                             
 [524] "                                return(d0) },"                                                                                          
 [525] "                      p = function(q, lower.tail = TRUE, log.p = FALSE ){"                                                              
 [526] "                                   if (!lower.tail) q <- -q"                                                                            
 [527] "                                   p0 <- ifelse( q <= 0,"                                                                               
