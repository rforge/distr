 [777] "                      p = function(q, lower.tail = TRUE, log.p = FALSE ){"                                                              
 [778] "                              if(!lower.tail) q<- -q"                                                                                   
 [779] "                              q <- pmin(pmax(q,-1),1)"                                                                                  
 [780] "                              p <- asin(q)/pi+1/2"                                                                                      
 [781] "                              if(log.p) p <- log(p)"                                                                                    
 [782] "                              return(p)},"                                                                                              
 [783] "                      q = function(p, lower.tail = TRUE, log.p = FALSE ){"                                                              
 [784] "                              if(log.p) p <- exp(p)"                                                                                    
 [785] "                              p1 <- p"                                                                                                  
 [786] "                              p1[p<0|p>1] <- 0.5"                                                                                       
 [787] "                              if(!lower.tail) p1 <- 1-p1"                                                                               
 [788] "                              q <- sin( (p1-1/2)*pi)"                                                                                   
 [789] "                              q[p<0|p>1] <- NA"                                                                                         
 [790] "                              q[.isEqual(p,0)] <- -1"                                                                                   
 [791] "                              q[.isEqual(p,1)] <-  1"                                                                                   
 [792] "                              return(q)},"                                                                                              
