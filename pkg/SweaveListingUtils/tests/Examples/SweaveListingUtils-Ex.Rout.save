 [393] "setClass(\"Distribution\","                                                                                                             
 [394] "          representation = representation("                                                                                             
 [395] "                      img = \"rSpace\","                                                                                                
 [396] "                      param = \"OptionalParameter\","                                                                                   
 [397] "                      r = \"function\","                                                                                                
 [398] "                      d = \"OptionalFunction\","                                                                                        
 [399] "                      p = \"OptionalFunction\","                                                                                        
 [400] "                      q = \"OptionalFunction\", # extended by P.R. 28-03-06"                                                            
 [401] "                      .withSim = \"logical\",   ## 'internal' slots => no"                                                              
 [402] "                      .withArith = \"logical\",  ## accessor/replacement functions"                                                     
 [403] "                      .logExact = \"logical\","                                                                                         
 [404] "                      .lowerExact = \"logical\","                                                                                       
 [405] "                      Symmetry = \"DistributionSymmetry\""                                                                              
 [406] "                      ),"                                                                                                               
 [407] "         prototype = prototype("                                                                                                        
 [408] "                     r = function(n){ rnorm(n, mean = 0, sd = 1) },"                                                                    
 [409] "                     d = function(x, log = FALSE)"                                                                                      
 [410] "                            { dnorm(x, mean = 0, sd = 1, log = log) },"                                                                 
 [411] "                     p = function(q, lower.tail = TRUE, log.p = FALSE )"                                                                
 [412] "                             { pnorm(q, mean = 0, sd = 1,"                                                                              
 [413] "                                     lower.tail = lower.tail, log.p = log.p) },"                                                        
 [414] "                     q = function(p, lower.tail = TRUE, log.p = FALSE )"                                                                
 [415] "                             { qnorm(p, mean = 0, sd = 1,"                                                                              
 [416] "                                     lower.tail = lower.tail, log.p = log.p) },"                                                        
 [417] "                     img = new(\"Reals\"),"                                                                                             
 [418] "                     param = NULL,"                                                                                                     
 [419] "                     .withArith = FALSE,"                                                                                               
 [420] "                     .withSim = FALSE,"                                                                                                 
 [421] "                     .logExact = FALSE,"                                                                                                
 [422] "                     .lowerExact = FALSE,"                                                                                              
 [423] "                     Symmetry = new(\"NoSymmetry\")"                                                                                    
 [424] "                     )"                                                                                                                 
 [425] "         )"                                                                                                                             
