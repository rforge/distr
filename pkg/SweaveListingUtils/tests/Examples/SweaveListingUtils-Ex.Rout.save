 [493] "## Class: Chi squared distribution"                                                                                                     
 [494] "setClass(\"Chisq\","                                                                                                                    
 [495] "          prototype = prototype("                                                                                                       
 [496] "                      r = function(n){ rchisq(n, df = 1, ncp = 0) },"                                                                   
 [497] "                      d = function(x, log = FALSE)"                                                                                     
 [498] "                                  { dchisq(x, df = 1, ncp = 0, log = log) },"                                                           
 [499] "                      p = function(q, lower.tail = TRUE, log.p = FALSE )"                                                               
 [500] "                                  { pchisq(q, df = 1, ncp = 0,"                                                                         
 [501] "                                           lower.tail = lower.tail,"                                                                    
 [502] "                                           log.p = log.p) },"                                                                           
