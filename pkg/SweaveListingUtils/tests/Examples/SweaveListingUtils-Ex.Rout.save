 [768] "          prototype = prototype("                                                                                                       
 [769] "                      r = function(n){ sin((runif(n)-.5)*pi) },"                                                                        
 [770] "                      d = function(x, log = FALSE){"                                                                                    
 [771] "                              x0 <- (abs(x)<1-.Machine$double.eps)"                                                                     
 [772] "                              x1 <- x^2*x0"                                                                                             
 [773] "                              d <-  x0/sqrt(1-x1)/pi"                                                                                   
 [774] "                              d[.isEqual(abs(x),1)] <- Inf"                                                                             
