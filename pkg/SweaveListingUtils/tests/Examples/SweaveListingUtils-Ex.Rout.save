 [314] "##"                                                                                                                                     
 [315] "################################"                                                                                                       
 [316] "## positive definite, symmetric matrices with finite entries"                                                                           
 [317] "setClass(\"PosSemDefSymmMatrix\", contains = \"matrix\","                                                                               
 [318] "            prototype = prototype(matrix(1)),"                                                                                          
 [319] "            validity = function(object){"                                                                                               
 [320] "                if(nrow(object) != ncol(object))"                                                                                       
 [321] "                    stop(\"no square matrix\")"                                                                                         
 [322] "                if(any(!is.finite(object)))"                                                                                            
 [323] "                    stop(\"inifinite or missing values in matrix\")"                                                                    
 [324] "                if(!isTRUE(all.equal(object, t(object), .Machine$double.eps^0.5,"                                                       
 [325] "                                     check.attributes = FALSE)))"                                                                       
 [326] "                    stop(\"matrix is not symmetric\")"                                                                                  
 [327] "                if(!all(eigen(object)$values > -100*.Machine$double.eps))"                                                              
 [328] "                   stop(\"matrix is (numerically) not positive semi - definite\")"                                                      
 [329] "               return(TRUE)"                                                                                                            
 [330] "            })"                                                                                                                         
 [331] ""                                                                                                                                       
 [332] "## positive definite, symmetric matrices with finite entries"                                                                           
 [333] "setClass(\"PosDefSymmMatrix\", contains = \"PosSemDefSymmMatrix\","                                                                     
 [334] "            validity = function(object){"                                                                                               
 [335] "               if(!all(eigen(object)$values > 100*.Machine$double.eps))"                                                                
 [336] "                   stop(\"matrix is (numerically) not positive definite\")"                                                             
 [337] "               valid <- getValidity(getClass(\"PosSemDefSymmMatrix\"))"                                                                 
 [338] "               valid(as(object, \"PosSemDefSymmMatrix\"))"                                                                              
 [339] "               return(TRUE)"                                                                                                            
 [340] "            })"                                                                                                                         
