 [765] "## Class: Arcsine distribution"                                                                                                         
 [766] "setClass(\"Arcsine\","                                                                                                                  
 [767] "          prototype = prototype("                                                                                                       
 [768] "                      r = function(n){ sin((runif(n)-.5)*pi) },"                                                                        
 [769] "                      d = function(x, log = FALSE){"                                                                                    
 [770] "                              x0 <- (abs(x)<1-.Machine$double.eps)"                                                                     
 [771] "                              x1 <- x^2*x0"                                                                                             
 [772] "                              d <-  x0/sqrt(1-x1)/pi"                                                                                   
 [773] "                              d[.isEqual(abs(x),1)] <- Inf"                                                                             
 [774] "                              if(log) d<- log(d)"                                                                                       
 [775] "                              return(d)},"                                                                                              
 [776] "                      p = function(q, lower.tail = TRUE, log.p = FALSE ){"                                                              
 [777] "                              if(!lower.tail) q<- -q"                                                                                   
 [778] "                              q <- pmin(pmax(q,-1),1)"                                                                                  
 [779] "                              p <- asin(q)/pi+1/2"                                                                                      
 [780] "                              if(log.p) p <- log(p)"                                                                                    
 [781] "                              return(p)},"                                                                                              
 [782] "                      q = function(p, lower.tail = TRUE, log.p = FALSE ){"                                                              
 [783] "                              if(log.p) p <- exp(p)"                                                                                    
 [784] "                              p1 <- p"                                                                                                  
 [785] "                              p1[p<0|p>1] <- 0.5"                                                                                       
 [786] "                              if(!lower.tail) p1 <- 1-p1"                                                                               
 [787] "                              q <- sin( (p1-1/2)*pi)"                                                                                   
 [788] "                              q[p<0|p>1] <- NA"                                                                                         
 [789] "                              q[.isEqual(p,0)] <- -1"                                                                                   
 [790] "                              q[.isEqual(p,1)] <-  1"                                                                                   
 [791] "                              return(q)},"                                                                                              
