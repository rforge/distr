 [541] "                                      ifelse( p <= 0.75   ,"                                                                            
 [542] "                                          qexp(2*p - 1, rate = 1),"                                                                     
 [543] "                                          qexp(2*(1-p), rate = 1,"                                                                      
 [544] "                                               lower.tail = FALSE)"                                                                     
 [545] "                                            )"                                                                                          
 [546] "                                         )"                                                                                             
 [547] "                                     )},"                                                                                               
