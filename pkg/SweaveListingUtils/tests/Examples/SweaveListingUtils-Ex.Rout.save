 [524] "                      p = function(q, lower.tail = TRUE, log.p = FALSE ){"                                                              
 [525] "                                   if (!lower.tail) q <- -q"                                                                            
 [526] "                                   p0 <- ifelse( q <= 0,"                                                                               
 [527] "                                                 0.5*pexp(-q, rate = 1,"                                                                
 [528] "                                                          lower.tail = FALSE),"                                                         
 [529] "                                                 0.5 + 0.5*pexp( q, rate = 1)"                                                          
 [530] "                                                    )"                                                                                  
