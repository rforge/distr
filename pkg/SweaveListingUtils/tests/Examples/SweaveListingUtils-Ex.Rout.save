 [317] "## positive definite, symmetric matrices with finite entries"                                                                           
 [318] "setClass(\"PosSemDefSymmMatrix\", contains = \"matrix\","                                                                               
 [319] "            prototype = prototype(matrix(1)),"                                                                                          
 [320] "            validity = function(object){"                                                                                               
 [321] "                if(nrow(object) != ncol(object))"                                                                                       
 [322] "                    stop(\"no square matrix\")"                                                                                         
 [323] "                if(any(!is.finite(object)))"                                                                                            
 [324] "                    stop(\"inifinite or missing values in matrix\")"                                                                    
 [325] "                if(!isTRUE(all.equal(object, t(object), .Machine$double.eps^0.5,"                                                       
 [326] "                                     check.attributes = FALSE)))"                                                                       
 [327] "                    stop(\"matrix is not symmetric\")"                                                                                  
 [328] "                if(!all(eigen(object)$values > -100*.Machine$double.eps))"                                                              
 [329] "                   stop(\"matrix is (numerically) not positive semi - definite\")"                                                      
 [330] "               return(TRUE)"                                                                                                            
 [331] "            })"                                                                                                                         
 [332] ""                                                                                                                                       
 [333] "## positive definite, symmetric matrices with finite entries"                                                                           
 [334] "setClass(\"PosDefSymmMatrix\", contains = \"PosSemDefSymmMatrix\","                                                                     
 [335] "            validity = function(object){"                                                                                               
 [336] "               if(!all(eigen(object)$values > 100*.Machine$double.eps))"                                                                
 [337] "                   stop(\"matrix is (numerically) not positive definite\")"                                                             
 [338] "               valid <- getValidity(getClass(\"PosSemDefSymmMatrix\"))"                                                                 
 [339] "               valid(as(object, \"PosSemDefSymmMatrix\"))"                                                                              
 [340] "               return(TRUE)"                                                                                                            
 [341] "            })"                                                                                                                         
