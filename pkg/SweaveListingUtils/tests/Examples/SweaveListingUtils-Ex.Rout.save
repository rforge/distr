 [481] "                                          },"                                                                                           
 [482] "                      q = function(p, lower.tail = TRUE, log.p = FALSE ){"                                                              
 [483] "                              qgamma(p, shape = 1, scale = 1,"                                                                          
 [484] "                                     lower.tail = lower.tail, log.p = log.p)"                                                           
 [485] "                                          },"                                                                                           
 [486] "                      param = new(\"GammaParameter\"),"                                                                                 
 [487] "                     .logExact = TRUE,"                                                                                                 
 [488] "                     .lowerExact = TRUE"                                                                                                
 [489] "                      ),"                                                                                                               
 [490] "          contains = \"ExpOrGammaOrChisq\""                                                                                             
