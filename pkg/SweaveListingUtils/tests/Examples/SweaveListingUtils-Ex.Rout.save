 [313] "##"                                                                                                                                     
 [314] "## matrix classes"                                                                                                                      
 [315] "##"                                                                                                                                     
 [316] "################################"                                                                                                       
 [317] "## positive definite, symmetric matrices with finite entries"                                                                           
 [318] "setClass(\"PosSemDefSymmMatrix\", contains = \"matrix\","                                                                               
 [319] "            prototype = prototype(matrix(1)),"                                                                                          
 [320] "            validity = function(object){"                                                                                               
 [321] "                if(nrow(object) != ncol(object))"                                                                                       
 [322] "                    stop(\"no square matrix\")"                                                                                         
 [323] "                if(any(!is.finite(object)))"                                                                                            
 [324] "                    stop(\"inifinite or missing values in matrix\")"                                                                    
 [325] "                if(!isTRUE(all.equal(object, t(object), .Machine$double.eps^0.5,"                                                       
 [326] "                                     check.attributes = FALSE)))"                                                                       
 [327] "                    stop(\"matrix is not symmetric\")"                                                                                  
 [328] "                if(!all(eigen(object)$values > -100*.Machine$double.eps))"                                                              
 [329] "                   stop(\"matrix is (numerically) not positive semi - definite\")"                                                      
 [330] "               return(TRUE)"                                                                                                            
 [331] "            })"                                                                                                                         
 [332] ""                                                                                                                                       
 [333] "## positive definite, symmetric matrices with finite entries"                                                                           
 [334] "setClass(\"PosDefSymmMatrix\", contains = \"PosSemDefSymmMatrix\","                                                                     
 [335] "            validity = function(object){"                                                                                               
 [336] "               if(!all(eigen(object)$values > 100*.Machine$double.eps))"                                                                
 [337] "                   stop(\"matrix is (numerically) not positive definite\")"                                                             
 [338] "               valid <- getValidity(getClass(\"PosSemDefSymmMatrix\"))"                                                                 
 [339] "               valid(as(object, \"PosSemDefSymmMatrix\"))"                                                                              
 [340] "               return(TRUE)"                                                                                                            
 [341] "            })"                                                                                                                         
 [342] ""                                                                                                                                       
 [343] ""                                                                                                                                       
 [344] "################################"                                                                                                       
 [345] "##"                                                                                                                                     
 [346] "## symmetry classes"                                                                                                                    
 [347] "##"                                                                                                                                     
 [348] "################################"                                                                                                       
 [349] ""                                                                                                                                       
 [350] "### from Matthias' thesis / ROptEst / moved from distrMod"                                                                              
 [351] ""                                                                                                                                       
 [352] "## class of symmetries"                                                                                                                 
 [353] "setClass(\"Symmetry\", representation(type = \"character\","                                                                            
 [354] "                                    SymmCenter = \"ANY\"),"                                                                             
 [355] "                     contains = \"VIRTUAL\")"                                                                                           
 [356] ""                                                                                                                                       
 [357] "## symmetry of distributions"                                                                                                           
 [358] "setClass(\"DistributionSymmetry\", contains = c(\"Symmetry\", \"VIRTUAL\"))"                                                            
 [359] ""                                                                                                                                       
 [360] "## no symmetry"                                                                                                                         
 [361] "setClass(\"NoSymmetry\", contains = \"DistributionSymmetry\","                                                                          
 [362] "            prototype = prototype(type = \"non-symmetric distribution\","                                                               
 [363] "                                  SymmCenter = NULL))"                                                                                  
 [364] ""                                                                                                                                       
 [365] "## elliptical symmetry"                                                                                                                 
 [366] "setClass(\"EllipticalSymmetry\", contains = \"DistributionSymmetry\","                                                                  
 [367] "            prototype = prototype(type = \"elliptically symmetric distribution\","                                                      
 [368] "                                  SymmCenter = numeric(0)))"                                                                            
 [369] ""                                                                                                                                       
 [370] "## spherical symmetry"                                                                                                                  
 [371] "setClass(\"SphericalSymmetry\", contains = \"EllipticalSymmetry\","                                                                     
 [372] "            prototype = prototype(type = \"spherically symmetric distribution\","                                                       
 [373] "                                  SymmCenter = numeric(0)))"                                                                            
 [374] ""                                                                                                                                       
 [375] "## list of symmetry types"                                                                                                              
 [376] "setClass(Class = \"DistrSymmList\","                                                                                                    
 [377] "            prototype = prototype(list(new(\"NoSymmetry\"))),"                                                                          
 [378] "            contains = \"list\","                                                                                                       
 [379] "            validity = function(object){"                                                                                               
 [380] "                nrvalues <- length(object)"                                                                                             
 [381] "                for(i in 1:nrvalues)"                                                                                                   
 [382] "                    if(!is(object[[i]], \"DistributionSymmetry\"))"                                                                     
 [383] "                        stop(\"element \", i, \" is no 'DistributionSymmetry'\")"                                                       
 [384] "                return(TRUE)"                                                                                                           
 [385] "            })"                                                                                                                         
 [386] ""                                                                                                                                       
 [387] ""                                                                                                                                       
 [388] "################################"                                                                                                       
 [389] "##"                                                                                                                                     
 [390] "## distribution classes"                                                                                                                
 [391] "##"                                                                                                                                     
 [392] "################################"                                                                                                       
 [393] ""                                                                                                                                       
 [394] "setClass(\"Distribution\","                                                                                                             
 [395] "          representation = representation("                                                                                             
 [396] "                      img = \"rSpace\","                                                                                                
 [397] "                      param = \"OptionalParameter\","                                                                                   
 [398] "                      r = \"function\","                                                                                                
 [399] "                      d = \"OptionalFunction\","                                                                                        
 [400] "                      p = \"OptionalFunction\","                                                                                        
 [401] "                      q = \"OptionalFunction\", # extended by P.R. 28-03-06"                                                            
 [402] "                      .withSim = \"logical\",   ## 'internal' slots => no"                                                              
 [403] "                      .withArith = \"logical\",  ## accessor/replacement functions"                                                     
 [404] "                      .logExact = \"logical\","                                                                                         
 [405] "                      .lowerExact = \"logical\","                                                                                       
 [406] "                      Symmetry = \"DistributionSymmetry\""                                                                              
 [407] "                      ),"                                                                                                               
 [408] "         prototype = prototype("                                                                                                        
 [409] "                     r = function(n){ rnorm(n, mean = 0, sd = 1) },"                                                                    
 [410] "                     d = function(x, log = FALSE)"                                                                                      
 [411] "                            { dnorm(x, mean = 0, sd = 1, log = log) },"                                                                 
 [412] "                     p = function(q, lower.tail = TRUE, log.p = FALSE )"                                                                
 [413] "                             { pnorm(q, mean = 0, sd = 1,"                                                                              
 [414] "                                     lower.tail = lower.tail, log.p = log.p) },"                                                        
 [415] "                     q = function(p, lower.tail = TRUE, log.p = FALSE )"                                                                
 [416] "                             { qnorm(p, mean = 0, sd = 1,"                                                                              
 [417] "                                     lower.tail = lower.tail, log.p = log.p) },"                                                        
 [418] "                     img = new(\"Reals\"),"                                                                                             
 [419] "                     param = NULL,"                                                                                                     
 [420] "                     .withArith = FALSE,"                                                                                               
 [421] "                     .withSim = FALSE,"                                                                                                 
 [422] "                     .logExact = FALSE,"                                                                                                
 [423] "                     .lowerExact = FALSE,"                                                                                              
 [424] "                     Symmetry = new(\"NoSymmetry\")"                                                                                    
 [425] "                     )"                                                                                                                 
 [426] "         )"                                                                                                                             
 [427] ""                                                                                                                                       
 [428] "## Class: UnivariateDistribution"                                                                                                       
